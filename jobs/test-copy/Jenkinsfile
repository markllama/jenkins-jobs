

node('container') {
    stage("Copy A File") {

        stage("run on master") {
            sh "pwd"
            sh "ls"
            sh "cp /jenkins/agent/.ssh/mlamouri.keytab ."
        }

        copy(
            srcFile: "kerberos.keytab",
            dstFile: "kerberos.keytab"
        )
    }
}
